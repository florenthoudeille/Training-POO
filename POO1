class Person {
    constructor(name, age) {
        this.name = name;
        this.age = age;
    }

    tellMyName() {
        console.log(`I am ${this.name}`);
    }

    tellMyAge() {
        console.log(`I am ${this.age} years old`);
    }
}

const Person1 = new Person('John', 40);
const Person2 = new Person('Mary', 35);

Person1.tellMyName();
Person1.tellMyAge();
Person2.tellMyName();
Person2.tellMyAge();
